package towerdefensegame;
import org.newdawn.slick.AppGameContainer;
import org.newdawn.slick.BasicGame;
import org.newdawn.slick.GameContainer;
import org.newdawn.slick.Graphics;
import org.newdawn.slick.SlickException;

public class SetupMap extends BasicGame{

	public SetupMap(String title) {
		super(title);

	}


	@Override
	public void init(GameContainer container) throws SlickException {

		
	}
	
	@Override
	public void update(GameContainer container, int updateTimer) throws SlickException {

		// Den här ska uppdatera när fienderna och tornen flyttar på sig
	}
	
	@Override
	public void render(GameContainer container, Graphics g) throws SlickException {

		// Ritar ut nya positionerna som skett i update.
	}

	
	public static void main(String[] args) throws SlickException{
		AppGameContainer app = new AppGameContainer(new Setup("Setup Test"));
		app.setDisplayMode(800,600,false);
		app.start();
		}
	
}
